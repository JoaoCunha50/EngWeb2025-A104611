extends layout

block content
    .w3-card-4
        header.w3-container.w3-teal
            h1 Detalhes do Contrato
        .w3-container
            ul.w3-ul.w3-card-4
                each val, key in contrato
                    if key !== '_id'
                        li
                            b #{key}: 
                            span #{val}
            .w3-container.w3-padding
                a.w3-button.w3-teal(href='/') Voltar
